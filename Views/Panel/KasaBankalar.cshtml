@model IEnumerable<Muhasebe.Models.KasaBanka>

@{
    ViewBag.Title = "KasaBankalar";
    Layout = "~/Views/Shared/_PanelLayout.cshtml";
}

<h2>KasaBankalar</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.HesapIsmi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HesapTuru)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DovizCinsi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AcilisBakiyesi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AcilisTarihi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Banka)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BankaSubesi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HesapNumarası)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Iban)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.HesapIsmi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HesapTuru)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DovizCinsi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AcilisBakiyesi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AcilisTarihi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Banka)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BankaSubesi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HesapNumarası)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Iban)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
